<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="15dp"
    android:background="@color/primary">

    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:textSize="18sp"
        android:text="@string/edit_profil"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="20dp"
        android:id="@+id/custom_profil_menu_dialog_button_edit_profile"
        android:textStyle="bold"/>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:textSize="18sp"
        android:text="@string/keluar"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/custom_profil_menu_dialog_button_edit_profile"
        android:layout_marginBottom="10dp"
        android:id="@+id/custom_profil_menu_dialog_button_keluar"
        android:textStyle="bold"/>

</RelativeLayout>