<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bottom_navigation_host"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="repo.pattimuradev.fsearch.view.fragment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_formBuatPengumumanFragment"
            app:destination="@id/formBuatPengumumanFragment" />
    </fragment>
    <fragment
        android:id="@+id/lombaFragment"
        android:name="repo.pattimuradev.fsearch.view.fragment.LombaFragment"
        android:label="fragment_lomba"
        tools:layout="@layout/fragment_lomba" >
        <action
            android:id="@+id/action_lombaFragment_to_formTambahLombaFragment"
            app:destination="@id/formTambahLombaFragment" />
    </fragment>
    <fragment
        android:id="@+id/penggunaFragment"
        android:name="repo.pattimuradev.fsearch.view.fragment.PenggunaFragment"
        android:label="fragment_pengguna"
        tools:layout="@layout/fragment_pengguna" />
    <fragment
        android:id="@+id/chatFragment"
        android:name="repo.pattimuradev.fsearch.view.fragment.ChatFragment"
        android:label="fragment_chat"
        tools:layout="@layout/fragment_chat" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="repo.pattimuradev.fsearch.view.fragment.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/formBuatPengumumanFragment"
        android:name="repo.pattimuradev.fsearch.view.fragment.FormBuatPengumumanFragment"
        android:label="fragment_form_buat_pengumuman"
        tools:layout="@layout/fragment_form_buat_pengumuman" >
        <action
            android:id="@+id/action_formBuatPengumumanFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/formTambahLombaFragment"
        android:name="repo.pattimuradev.fsearch.view.fragment.FormTambahLombaFragment"
        android:label="fragment_form_tambah_lomba"
        tools:layout="@layout/fragment_form_tambah_lomba" >
        <action
            android:id="@+id/action_formTambahLombaFragment_to_lombaFragment"
            app:destination="@id/lombaFragment" />
    </fragment>
</navigation>